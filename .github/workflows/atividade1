programa
{
	
	funcao inicio()
	{
		inteiro vetor[10] = {2, 5, 1, 3, 4, 9, 7, 8, 10, 6}
		para (inteiro i=0; i<10; i++){
			para (inteiro l=i +1; l<10; l++)
			{
				se (vetor[i] < vetor[l]){
					inteiro temp = vetor[i]
					vetor[i] = vetor[l]
					vetor[l] = temp
				}
			}
		}

		      escreva("Ordenado em ordem decrescente: ")
		      para (inteiro i=0; i<10; i++)
		      {
		      	escreva(vetor[i], " ")
		      }
	}
}
